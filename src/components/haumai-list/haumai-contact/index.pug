section.haumai-contact
	.container
		.block-title
			h1.head-title Máy hút liên tục
			.contact
				span.ri-phone-fill
				a(href='javascript:void(0)' data-fancybox="" data-src="#wrap-form-popup") Liên hệ
		.haumai-slide-swiper-wrapper
			.nav-arrow-prev
				em.material-icons arrow_back_ios
			.nav-arrow-next
				em.material-icons arrow_forward_ios
			.swiper-container
				.swiper-wrapper
					-for(i=0;i<6;i++)
						.swiper-slide
							.wrapper
								.card-img.zoom-in
									a(href="#")
										img(src="./img/home/h-3.png", alt="")
								.title
									a(href="#") Máy hút chân không WP-3090S

