#wrap-form-popup
	.title-popup Liên hệ tư vấn
	.row
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text', placeholder="Họ tên *")
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text',placeholder="Số điện thoại *")
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text',placeholder="Email *")
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text',placeholder="Tên sản phẩm")
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text',placeholder="Tên công ty")
		.form-group.col-12.col-md-6.col-6
			input.form-control(type='text',placeholder="Chức vụ")
		.form-group.col-12
			textarea#inpputTextArea.form-control(rows='3',placeholder="Nội dung")
	.frm-btnwrap
		.frm-btn
			input(type="submit" value ="GỬI")
